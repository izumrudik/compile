#!jararaca.py -il

struct Point {
	x:int
	y:int
}
struct Point {
	x:int
	y:int
	z:int
}
fun main() {
	x = TokenType.KEYWORD("match")
	match @x as value {
		KEYWORD -> {
			put`found a keyword, value is '{value}'`
		}
		default -> {
			put`default`
		}
	}
}


enum TokenType {
	KEYWORD:str
	IDENTIFIER:int
	INTEGER#:complicated_struct
	FLOAT
	STRING
	OPERATOR
	PUNCTUATION
	COMMENT
	WHITESPACE
	EOF
}


#!./lang.py -rs
fun printd a:int -> void {
	if a < 10 {
		print(str(1, ptr('0123456789')+a))
	} else {
	  	printd(a//10)
	  	printd(a%10)
	  }
}

fun main {
	a:int = 1
	b:int = 1
	c:int = 0
	idx:int = 2
	while idx < 30 {
		c = a+b
		printd(idx)
		print(': ')
		printd(c)
		print('\n')
		a = b
		b = c
		idx = idx + 1
	}
}

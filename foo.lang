#!./lang.py -rs
fun printd a:int -> void {
	if a < 10 {
		print(str(1, ptr('0123456789')+a))
	} else {
	  	printd(a//10)
	  	printd(a%10)
	  }
}



fun fib idx:int -> int{
	if idx <= 2 {
		return 1
	}
	a:int = fib(idx-1)
	b:int = fib(idx-2)
	return a+b
	
}



fun main {
	printd(fib(35))
	print('\n')
}

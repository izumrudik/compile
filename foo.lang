#!./lang.py -rs
fun printd a:int -> void {
	if a < 10 {
		print(str(1, ptr('0123456789')+a))
	} else {
	  	printd(a//10)
	  	printd(a%10)
	  }
}



fun fib_rec idx:int -> int{
	if idx<1 {
		return 0
	}
	a:int = fib_rec(idx-1)
	b:int = fib_rec(idx-2)
	return a+b
	
}


fun main {

	printd(fib_rec(5))
}

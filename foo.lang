fun printd a:int -> void {
	
	if a < 10 {
		print(
			str(
				1,
				int_to_ptr(
					ptr_to_int(
						ptr(
							'0123456789'
						)
					)+a
				)
			)
		)
	}
	else {
	  	printd(a//10)
	  	printd(a%10)
	  }
}

	save(row[N-2], 1c)
	i:int
	pat:int
	pat_new:int
	idx:int = 0
	while idx < N-2 {
		i=0
		while i < N {
			if @row[i] == 1c {
				puts('*')
			} else {
				puts(' ')
			}
			i = i + 1
		}
		putendl()

		pat = int(@row[0])<<1 + int(@row[1]) #

		i = 1
		while i < N-2 {
			pat = (pat<<1 + int(@row[i+1])) and 7
			save(row[i], char(nth_bit(110, pat)))
			i = i+1
		}
		idx=idx+1
	}
}